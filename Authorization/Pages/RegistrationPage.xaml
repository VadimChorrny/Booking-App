<Page x:Class="RentServiceApp.Pages.RegistrationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RentServiceApp.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="400"
      Title="RegistrationPage" Background="White" >

    <Grid>
        <StackPanel Margin="40">
            <TextBlock Text="Enter login" FontSize="30" Margin="0 30 0 0" />
            <TextBox FontSize="30" Name="txtLogin" />
            <TextBlock Text="Enter password" FontSize="30" Margin="0 30 0 0"/>
            <TextBox FontSize="30" Name="txtPassword" />
            <Button Content="GO!" FontSize="30" Margin="0 70 0 0" Click="Button_Click" />
        </StackPanel>
    </Grid>
</Page>
